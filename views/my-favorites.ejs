<h1 class="page-title">Welcome <%= user.username %> </h1>
<% if(user.myProfessionals.length > 0) {%> 
<h2>My Stores:</h2>
<ul class="favorites-list">
<% user.myProfessionals.forEach(element => { %>
  <li><a href="/professionals/<%= element._id %>"><%= element.username %> </a></li>
<% }) %>
</ul>
<% }else { %>
  <h3>You don't have stores, add one </h3>
<% }%> 
<%- include('partials/navbar.ejs') %>